                      Branch:
                        name: else
                        bits: []
                        children:
                          - Text: "\n                    <span>User</span>\n                "
                  - Block:
                      Closing:
                        name: endif
                        bits: []
          - Text: "\n            "
          - Block:
              Closing:
                name: endif
                bits: []
  - Text: "\n        </div>\n    </body>\n</html>"
